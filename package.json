{
  "//": "The production flag is added in the jenkins build script.",
  "name": "kursutveckling-api",
  "version": "2.0.0",
  "description": "Collect data for course development and history page, used by kursutveckling-web and kursutveckling-admin-web.",
  "main": "app.js",
  "scripts": {
    "codecheck": "pretty-quick",
    "test-ci": "is-ci \"test\" \"test:watch\"",
    "test": "NODE_ENV=test jest test/unit/*.js",
    "test:watch": "NODE_ENV=test jest test/unit/*.js --watch",
    "start": "NODE_ENV=production node app.js",
    "start-dev": "cross-env NODE_ENV=development nodemon app.js"
  },
  "dependencies": {
    "body-parser": "^1.19.0",
    "component-registry": "^1.3.1",
    "cookie-parser": "^1.4.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-handlebars": "^4.0.4",
    "kth-node-access-log": "^0.2.0",
    "kth-node-api-common": "^1.0.1",
    "kth-node-api-key-strategy": "^1.0.9",
    "kth-node-configuration": "^2.0.1",
    "kth-node-express-routing": "^2.0.3",
    "kth-node-log": "^3.1.3",
    "kth-node-mongo": "^3.3.7",
    "kth-node-monitor": "^1.1.8",
    "kth-node-response": "KTH/kth-node-response.git#v1.0.0",
    "kth-node-server": "^3.1.2",
    "mongoose": "^5.11.11",
    "passport": "^0.4.1",
    "safe-utils": "0.2.1",
    "swagger-ui-dist": "^3.39.0"
  },
  "devDependencies": {
    "babel-eslint": "^10.1.0",
    "babel-jest": "^26.6.3",
    "chai": "^4.2.0",
    "concurrently": "^2.2.0",
    "cross-env": "^7.0.3",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.3.1",
    "husky": "^3.0.9",
    "is-ci-cli": "^2.1.2",
    "jest": "^26.6.3",
    "nodemon": "^2.0.7",
    "prettier": "^2.2.1",
    "pretty-quick": "^2.0.1",
    "proxyquire": "^2.1.3"
  },
  "engines": {
    "node": "12.0.0"
  },
  "husky": {
    "hooks": {}
  },
  "prettier": {
    "trailingComma": "es5",
    "tabWidth": 2,
    "semi": false,
    "singleQuote": true,
    "printWidth": 120,
    "arrowParens": "avoid"
  }
}
